{
  "tests": [
    {
      "name": "Create homework set with valid data",
      "method": "POST",
      "headers": {
        "X-Auth-Token": "valid_teacher_token"
      },
      "body": {
        "title": "Test Homework",
        "description": "Test description",
        "task_ids": [1, 2, 3]
      },
      "expectedStatus": 200,
      "expectedBody": {
        "success": true,
        "homework_set": {
          "id": "number",
          "title": "string"
        }
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Reject without token",
      "method": "POST",
      "body": {
        "title": "Test",
        "task_ids": [1]
      },
      "expectedStatus": 401
    }
  ]
}
